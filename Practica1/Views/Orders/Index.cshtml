@model IEnumerable<Practica1.Models.Orders>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Customers)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salesman_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Order_date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Customer_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salesman_id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Order_date)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Order_id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Order_id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Order_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
